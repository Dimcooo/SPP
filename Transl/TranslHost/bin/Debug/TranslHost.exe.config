<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <services>
      <service name="Transl.Transl" behaviorConfiguration="mexBehavoir">
        <endpoint address="Transl.Transl" binding="basicHttpBinding" contract="Transl.ITransl"></endpoint>
        <endpoint address="Transl.Transl" binding="netTcpBinding" contract="Transl.ITransl"></endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract ="IMetadataExchange"></endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8082"/>
            <add baseAddress="net.tcp://localhost:45618"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mexBehavoir">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>